{
  "id": "com.docBoard",
  "name": "documentBoard",
  "version": "1.0.0",
  "host": [
    {
      "app": "PS",
      "minVersion": "23.0.0",
      "data":{
        "apiVersion": 2
      }
    }
  ],
  "main": "index.html",
  "manifestVersion": 4,
  "entrypoints": [
      {
      "type": "panel",
      "id": "mainList",
      "label": {
        "default": "mainList"
      },
      "minimumSize": {
        "width": 120,
        "height": 140
      },
      "maximumSize": {
        "width": 1200,
        "height": 10000
      },
      "preferredDockedSize": {
        "width": 150,
        "height": 200
      },
      "preferredFloatingSize": {
        "width": 300,
        "height": 200
      }
      ,
      "icons": [
        { "width": 23, "height": 23, "path": "./icons/toolIconDark.png", "scale": [ 1, 2 ], "theme": [ "darkest", "dark", "medium"]},
        { "width": 23, "height": 23, "path": "./icons/toolIconLight.png", "scale": [ 1, 2 ], "theme": [ "lightest", "light"]}
      ]
    },
    {
      "type": "panel",
      "id": "export",
      "label": {
        "default": "export"
      },
      "minimumSize": {
        "width": 120,
        "height": 140
      },
      "maximumSize": {
        "width": 1200,
        "height": 10000
      },
      "preferredDockedSize": {
        "width": 150,
        "height": 200
      },
      "preferredFloatingSize": {
        "width": 300,
        "height": 200
      }
      ,
      "icons": [
        { "width": 23, "height": 23, "path": "./icons/toolIconDark.png", "scale": [ 1, 2 ], "theme": [ "darkest", "dark", "medium"]},
        { "width": 23, "height": 23, "path": "./icons/toolIconLight.png", "scale": [ 1, 2 ], "theme": [ "lightest", "light"]}
      ]
    },
    {
      "type": "panel",
      "id": "detail",
      "label": {
        "default": "detailList"
      },
      "minimumSize": {
        "width": 800,
        "height": 500
      },
      "maximumSize": {
        "width": 1300,
        "height": 10000
      },
      "preferredDockedSize": {
        "width": 150,
        "height": 200
      },
      "preferredFloatingSize": {
        "width": 300,
        "height": 200
      }
      ,
      "icons": [
        { "width": 23, "height": 23, "path": "./icons/toolIconDark.png", "scale": [ 1, 2 ], "theme": [ "darkest", "dark", "medium"]},
        { "width": 23, "height": 23, "path": "./icons/toolIconLight.png", "scale": [ 1, 2 ], "theme": [ "lightest", "light"]}
      ]
    }
  ],
  "icons": [
    { "width": 48, "height": 48, "path": "./icons/dark.png", "scale": [ 1, 2 ], "theme": [ "all"]}
  ]
}